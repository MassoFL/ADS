# Configuration pour l'application de génération de publicités IA

# Clé API Mistral AI (obligatoire)
# Obtenez votre clé sur: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here

# Clé secrète Flask (optionnel, généré automatiquement si non défini)
SECRET_KEY=your_secret_key_here

# Configuration Flask (optionnel)
FLASK_ENV=development
FLASK_DEBUG=True